<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Events</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/styles/bootstrap.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/styles/app.css")" />
</head>
<body>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>
